	note = {Publisher: SAGE Publications Inc STM},
	keywords = {COVID-19, infectious diseases, mathematical modeling, uncertainty, validation},
	pages = {379--385},
	file = {SAGE PDF Full Text:C\:\\Users\\kaths\\Zotero\\storage\\Q4WFFG7D\\James et al. - 2021 - The Use and Misuse of Mathematical Modeling for In.pdf:application/pdf},
}

