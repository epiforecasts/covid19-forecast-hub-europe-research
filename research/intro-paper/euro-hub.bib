@article{bracher_evaluating_2021,
	title = {Evaluating epidemic forecasts in an interval format},
	volume = {17},
	issn = {1553-7358},
	url = {https://journals.plos.org/ploscompbiol/article?id=10.1371/journal.pcbi.1008618},
	doi = {10.1371/journal.pcbi.1008618},
	abstract = {For practical reasons, many forecasts of case, hospitalization, and death counts in the context of the current Coronavirus Disease 2019 (COVID-19) pandemic are issued in the form of central predictive intervals at various levels. This is also the case for the forecasts collected in the COVID-19 Forecast Hub (https://covid19forecasthub.org/). Forecast evaluation metrics like the logarithmic score, which has been applied in several infectious disease forecasting challenges, are then not available as they require full predictive distributions. This article provides an overview of how established methods for the evaluation of quantile and interval forecasts can be applied to epidemic forecasts in this format. Specifically, we discuss the computation and interpretation of the weighted interval score, which is a proper score that approximates the continuous ranked probability score. It can be interpreted as a generalization of the absolute error to probabilistic forecasts and allows for a decomposition into a measure of sharpness and penalties for over- and underprediction.},
	language = {en},
	number = {2},
	urldate = {2021-07-15},
	journal = {PLOS Computational Biology},
	author = {Bracher, Johannes and Ray, Evan L. and Gneiting, Tilmann and Reich, Nicholas G.},
	month = feb,
	year = {2021},
	note = {Publisher: Public Library of Science},
	keywords = {Forecasting, Public and occupational health, Probability distribution, Pandemics, Binomials, COVID 19, Epidemiological methods and statistics, Instrument calibration},
	pages = {e1008618},
	file = {Full Text PDF:C\:\\Users\\kaths\\Zotero\\storage\\MXU3F99C\\Bracher et al. - 2021 - Evaluating epidemic forecasts in an interval forma.pdf:application/pdf;Snapshot:C\:\\Users\\kaths\\Zotero\\storage\\VM2K6CFR\\article.html:text/html},
}

@misc{northcott_when_2017,
	type = {Published {Article} or {Volume}},
	title = {When are purely predictive models best?},
	url = {https://www.degruyter.com/downloadpdf/j/disp.2017.9.issue-47/disp-2017-0021/disp-2017-0021.pdf},
	abstract = {Can purely predictive models be useful in investigating causal systems? I argue “yes”. Moreover, in many cases not only are they useful, they are essential. The alternative is to stick to models or mechanisms drawn from well-understood theory. But a necessary condition for explanation is empirical success, and in many cases in social and field sciences such success can only be achieved by purely predictive models, not by ones drawn from theory. Alas, the attempt to use theory to achieve explanation or insight without empirical success therefore fails, leaving us with the worst of both worlds—neither prediction nor explanation. Best go with empirical success by any means necessary. I support these methodological claims via case studies of two impressive feats of predictive modelling: opinion polling of political elections, and weather forecasting.},
	language = {en},
	urldate = {2021-07-17},
	journal = {Disputatio},
	author = {Northcott, Robert},
	month = dec,
	year = {2017},
	note = {Pages: 631-656},
	file = {Full Text PDF:C\:\\Users\\kaths\\Zotero\\storage\\2WWRYLDH\\Northcott - 2017 - When are purely predictive models best.pdf:application/pdf;Snapshot:C\:\\Users\\kaths\\Zotero\\storage\\3JDJFCKF\\15383.html:text/html},
}

@article{reich_accuracy_2019,
