	pmcid = {PMC6708259},
	keywords = {Humans, Forecasting, Epidemics, Disease Outbreaks, Surveillance, Population Surveillance, Infectious diseases, Models, Statistical, Data Collection, Public Health, Disease modeling, Epidemic management/response, Machine Learning},
	pages = {268--275},
